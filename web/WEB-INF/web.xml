<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>demo</servlet-name>
        <servlet-class>demo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>diseases</servlet-name>
        <servlet-class>diseases</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>all_diseases</servlet-name>
        <servlet-class>admin.all_diseases</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_symptoms</servlet-name>
        <servlet-class>admin.add_symptoms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processAddSymptom</servlet-name>
        <servlet-class>processAddSymptom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>demo</servlet-name>
        <url-pattern>/demo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>all_diseases</servlet-name>
        <url-pattern>/all_diseases</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_symptoms</servlet-name>
        <url-pattern>/add_symptoms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processAddSymptom</servlet-name>
        <url-pattern>/processAddSymptom</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
